{
  "name": "ergonode/importer-verto",
  "type": "ergonode-module",
  "description": "Ergonode - Importer Verto",
  "homepage": "https://ergonode.com",
  "license": "OSL-3.0",
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "ext-pdo": "*",
    "ext-zip": "*",
    "ergonode/importer": "^1.1.0",
    "ergonode/product": "^1.1.0",
    "ramsey/uuid": "^3.8",
    "ramsey/uuid-doctrine": "^1.5",
    "league/csv": "^9.6",
    "symfony/form": "^4.4.11",
    "symfony/translation": "^4.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Ergonode\\ImporterVerto\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergonode\\ImporterVerto\\Tests\\": "tests/"
    }
  }
}
